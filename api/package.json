{
  "name": "@emperione/api",
  "version": "0.1.0",
  "private": true,
  "description": "Backend API server for Emperione",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "lint": "eslint src --ext .ts"
  },
  "keywords": ["api", "express", "discord"],
  "author": "Emperione Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "typescript": "^5.4.2",
    "ts-node-dev": "^2.0.0",
    "prisma": "^5.7.0",
    "@types/express": "^4.17.21",
    "eslint": "^8.45.0",
    "@types/node": "^20.5.6",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  }
}

